@mixin boxShadow($horz: 4px, $vert: 3px, $depth: 5px, $color: rgba(0, 0, 0, 1))
{
  -webkit-box-shadow: $horz $vert $depth $color;
  -moz-box-shadow: $horz $vert $depth $color;
  box-shadow: $horz $vert $depth $color;
}

@mixin borderRadEight($radius: 8px)
{
	-webkit-border-radius: $radius;
	-moz-border-radius: $radius;
	-ms-border-radius: $radius;
	-o-border-radius: $radius;
	border-radius: $radius;
}

